{
    "debug": false,
    "http": {
        "enabled": true,
        "listen": "%%QUERY_HTTP%%"
    },
    "hosts": {
        "enabled": true,
        "interval": 60
    },
    "contacts": {
        "enabled": true,
        "interval": 3600
    },
    "gin_http": {
        "enabled":  true,
        "listen":   "%%GIN_HTTP%%"
    },
    "graph": {
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            "graph-00": "%%GRAPH_RPC%%"
        }
    },
    "grpc": {
      "enabled":  true,
      "port": 1235
    },
    "api": {
        "name": "api_owl",
        "token": "fastweb_owl",
        "event": "https://cdnboss-api.fastweb.com.cn/Monitor/add_zabbix_event",
        "map": "https://cdnboss-api.fastweb.com.cn/Base/platform/get_ip",
        "geo": "https://cdnboss-api.fastweb.com.cn/pop/get_area",
        "uplink": "https://cdnboss-api.fastweb.com.cn/base/get_uplink_list",
        "query": "http://127.0.0.1:9966",
        "dashboard": "http://127.0.0.1:8081",
        "max": 1000
    },
    "graphdb": {
        "addr": "%%MYSQL%%/graph?charset=utf8&parseTime=True&loc=Local",
        "idle": 10,
        "max": 100,
        "limit": -1
    },
    "db": {
        "addr": "%%MYSQL%%/falcon_portal?charset=utf8&loc=Asia%2FTaipei",
        "idle": 10,
        "max": 100
    },
    "bossdb": {
        "addr": "%%MYSQL%%/boss?charset=utf8&loc=Asia%2FTaipei",
        "idle": 10,
        "max": 100
    },
    "nqm": {
        "addr": "%%MYSQL%%/gz_nqm?charset=utf8&loc=Asia%2FTaipei",
        "idle": 10,
        "max": 100
    },
    "local": "http://localhost:9966",
    "nqmlog": {
        "jsonrpcUrl": "http://127.0.0.1:6171/jsonrpc"
    },
    "fe": "http://127.0.0.1:1235"
}
